        "@types/body-parser": "^1.19.0",
        "@types/cors": "^2.8.8",
        "@types/express": "^4.17.8",
        "@types/formidable": "^1.0.32",
        "@types/jest": "^26.0.15",
        "@types/supertest": "^2.0.10",
        "@typescript-eslint/eslint-plugin": "^5.28.0",
        "@typescript-eslint/parser": "^5.28.0",
        "babel-eslint": "^10.1.0",
        "babel-plugin-module-resolver": "^4.0.0",
