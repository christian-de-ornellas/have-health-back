        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-simple-import-sort": "^5.0.3",
        "eslint-plugin-standard": "^4.0.1",
        "husky": "^8.0.1",
        "jest": "^26.6.1",
        "lint-staged": "^13.0.2",
        "prettier": "^2.7.1",
        "supertest": "^6.0.0",
        "ts-jest": "^26.4.3",
        "ts-node-dev": "^1.0.0-pre.49",
        "tsconfig-paths": "^4.0.0",
        "typescript": "^3.9.5"
